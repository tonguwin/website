 <style>
    .marquee-container {
      overflow: hidden;
      white-space: nowrap;
      position: relative;
      width: 300%; /* Adjust width as needed */

    }
    
    .marquee {
      display: inline-block;
      white-space: nowrap;
      animation: marquee-bounce 28s linear infinite;
    }

    .marquee svg {
      display: inline-block; /* Ensure SVGs are displayed inline */
      margin-right: 10px; /* Optional: space between SVGs */
    }

    @keyframes marquee-bounce {
      0% {
        transform: translateX(0);
      }
      50% {
        transform: translateX(calc(-100% + 100vw));
      }
      100% {
        transform: translateX(0);
      }
    }
  </style>

